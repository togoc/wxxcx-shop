/* components/SearchInput/SearchInput.wxss */

@import "../../styles/icons.wxss";
.search_input {
    height: 90rpx;
    padding: 10rpx;
    display: flex;
    justify-content: center;
    background-color: var(--themeColor);
}

.search_input navigator {
    background-color: #fff;
    display: flex;
    justify-content: center;
    border-radius: 15rpx;
    width: 100%;
    height: 100%;
    align-items: center;
}

.search_input text {
    color: red;
    font-size: 24rpx;
    margin: 2px;
}